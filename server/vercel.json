{
    "version": 2,
    "routes": [
        {
            "src": "/api/health",
            "dest": "/api/health.ts",
            "methods": ["GET"]
        },
        {
            "src": "/api/search",
            "dest": "/api/search.ts"
        },
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        }
    ]
}
